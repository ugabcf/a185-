#include <iostream>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	int a,b,c,d;
	cin>>a;
	b=a/10;                               //b為十位數
	c=a%10;                               //c為個位數
	d=0;                                  
	if(c==2||c==5||c==8) d=d+200;
	if(b%2==1) d=d+100;
	if(b==c) d=d+50;
	else d==0;
	cout<<d;
	
}
